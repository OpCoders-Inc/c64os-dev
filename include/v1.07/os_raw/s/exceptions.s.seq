;----[ EXCEPTIONS.S ]-------------------REGSTORE = $C8     ;Á/Ø/Ù +0/+1/+2BACKREGS = $03BC   ;ÔEMP ÓTASHES Á/Ø/ÙREADREGS = $03C3   ;ÒEADS Á/Ø/Ù ÂACK ÉNTRY¤     = $03CA   ;ÅXCEPTION ÈANDLEREXCEPTION = $03DF  ;ÒAISE AN ÅXCEPTIONEXCINDEX = $C7     ;ÅXCEPTIONS ÔAB ÉNDEXEXCADDR  = $0E;$0F ;ÅXCEPTION ÁDDRESSTRY      .MACRO ;CATCH         JSR BACKREGS         LDA #<\1         LDX #>\1         JSR TRY¤         .ENDM                 ;10 BYTES;ÕSE EXITTRY IF THERE IS NO CATCH BLOCK.EXITTRY  .MACRO         DEC EXCINDEX           ;2 BYTES         .ENDM;ÕSE ENDTRY TO SKIP THE CATCH BLOCK.ENDTRY   .MACRO ;ENDCATCH         #EXITTRY         JMP \1         .ENDM                  ;5 BYTES